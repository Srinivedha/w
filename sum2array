import java.io.*;
import java.util.*;
class sum2array
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int arr[]=new int[a];
        int b=s.nextInt();
        for(int i=0;i<a;i++)
        {
         arr[i]=s.nextInt();   
        }
        int sum=0;
        for(int i=0;i<b;i++)
        {
          sum=sum+arr[i];  
        }
        System.out.print(sum);
    }
}
